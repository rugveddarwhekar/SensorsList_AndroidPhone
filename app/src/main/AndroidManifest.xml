<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.comp528project3sensorslist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Temperature"></activity>
        <activity android:name=".Pressure" />
        <activity android:name=".Proximity" />
        <activity android:name=".Magnet" />
        <activity android:name=".Light" />
        <activity android:name=".Gyroscope" />
        <activity android:name=".Gravity" />
        <activity android:name=".LinearAcceleration" />
        <activity android:name=".Accelerometer" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>